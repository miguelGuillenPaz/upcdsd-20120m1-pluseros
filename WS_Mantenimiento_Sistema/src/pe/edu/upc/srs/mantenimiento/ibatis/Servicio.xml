<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap
PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
"http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="Servicio"   >

    <typeAlias alias="servicio" 
               type="pe.edu.upc.srs.reserva.beans.ServicioDTO"/>

	<procedure id="sp_registrar_servicio" parameterClass="pe.edu.upc.srs.reserva.beans.ServicioDTO">
		{call sp_registrar_servicio(?,?,?,?)}
	</procedure>
	
	<procedure id="sp_modificar_servicio" parameterClass="pe.edu.upc.srs.reserva.beans.ServicioDTO">
		{call sp_modificar_servicio(?,?,?,?)}
	</procedure>
	
	<procedure id="sp_eliminar_servicio" parameterClass="java.lang.Integer">
		{call sp_eliminar_servicio(#VALUE#)}
	</procedure>
	
	<procedure id="sp_obtenerservicios_servicio" parameterClass="pe.edu.upc.srs.reserva.beans.ServicioDTO">
		{call sp_obtenerservicios_servicio}
	</procedure>
	
	<procedure id="sp_buscar_servicio" parameterClass="java.lang.Integer">
		{call sp_buscar_servicio(#VALUE#)}
	</procedure>
	
</sqlMap>
 